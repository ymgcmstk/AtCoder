  else return a;
}

int input_value(){
  int a;
  cin >> a;
  return a;
}

void input_array(int * p, int a){
  rep(i, a){
    cin >> p[i];
  }
}

void input_vector(vector<int> * p, int a){
  rep(i, a){
    int input;
    cin >> input;
    p -> push_back(input);
  }
}

template <typename T>
void output(T a, int precision) {
  if(precision > 0){
    cout << setprecision(precision)  << a << "\n";
  } else {
    cout << a << "\n";
  }
}

int main() {
  int N;
  cin >> N;
  int Ax[N], Ay[N];
  int Bx[N], By[N];
  int sumx, sumy, Av, Bv, tempx, tempy;
  sumx = 0;
  sumy = 0;
  Av = 0;
  rep(i, N) {
    cin >> tempx >> tempy;
    sumx += tempx;
    sumy += tempy;
    Av += tempx * tempx + tempy * tempy;
  }
  Av -= (sumx * sumx + sumy * sumy);
  sumx = 0;
  sumy = 0;
  Bv = 0;
  rep(i, N) {
    cin >> tempx >> tempy;
    sumx += tempx;
    sumy += tempy;
    Bv += tempx * tempx + tempy * tempy;
  }
  Bv -= (sumx * sumx + sumy * sumy);
  cout << sqrt(Bv / Av) << endl;
  return 0;
}
