#define repd(i,a,b) for (int i=(a);i<(b);i++)
#define mrepd(i,a,b) for (int i=(b-1);i>=(a);i--)
#define rep(i,n) repd(i,0,n)
#define mrep(i,n) mrepd(i,0,n)
#define LL long long
#define MOD 1000000007
using namespace std;

template <typename T> void input_array(T * p){
  int a = sizeof(p) / sizeof(p[0]);
  rep(i, a){
    cin >> p[i];
  }
}

void input_vector(vector<int> * p, int a){
  rep(i, a){
    int input;
    cin >> input;
    p -> push_back(input);
  }
}

template <typename T>
void output(T a, int precision) {
  if (precision > 0) {
    cout << setprecision(precision)  << a << "\n";
  } else {
    cout << a << "\n";
  }
}

int main() {
  int N, K;
  cin >> N >> K;
  int s[N];
  rep(i, N) cin >> s[i];
  rep(i, N) {
    if (s[i] == 0) {
      cout << N << endl;
      return 0;
    }
  }
  int cur_max = 0;
  int cur_prod = s[0];
  if (cur_prod < K) cur_max = 1;
  int front = 1; // next
  int back = 0; // next
  while (front < N || front - back > cur_max) {
    if (cur_prod <= K && front < N) {
      cur_max = max(cur_max, front - back);
      cur_prod *= s[front];
      front++;
    } else if (front == back) {
      cur_prod *= s[front];
      front++;
    } else {
      cur_prod /= s[back];
      back++;
    }
  }
  cout << cur_max << endl;
  // source code
  return 0;
}
